{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended"
        // "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
        // "prettier"
    ],
    "rules": {
        "linebreak-style": ["off", "windows"],
        "react/no-children-prop": 0,
        "react/no-find-dom-node": 0,
        "react/prop-types": 0
    }
}
